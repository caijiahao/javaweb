public boolean isWake(int t){
	for(int i =0;i<A.length;i++){
		for(int j =0;j<B.length;j++){
			if(A[i][1]>(B[j][0]+t)&&B[j][1]+t>=A[i][0]){
				return true;
			}
		}
	}
	return false;
}

