package wangyi.second;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
    public static int caclulateRight(int num){
    	int count=0;
    	while(num>0){
    		if(num%2==1){
    			count++;
    		}
    		num/=2;
    	}
    	return count;
    }
}
